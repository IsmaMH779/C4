<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>P Categoria</title>

</head>

<body>
    <h1>
        <%= cat %>
    </h1>
    <table>
        <tr>
            <th>nombre</th>
            <th>precio</th>
            <th>stock</th>
            <th>Fabricante</th>
        </tr>
        <% productos.forEach(p=> { %>
            <tr>
                <td>
                    <%= p.art %>
                </td>
                <td>
                    <%= p.precio %>
                </td>
                <td>
                    <%= p.stock %>
                </td>
                <td>
                    <%= p.nombre %>
                </td>
                <td>
                    <a href="/editar/<%= p.pid %>">Editar</a>
                    <a href="/eliminar/<%= p.pid %>">Eliminar</a>
                </td>
            </tr>
            <% }) %>
    </table>

</body>

</html>